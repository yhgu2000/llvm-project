//===----------------------------------------------------------------------===//
// Registers
//===----------------------------------------------------------------------===//

// 后端框架好像需要至少一个寄存器，所以定义一个意思意思
def R0: Register<"r0">;

//===----------------------------------------------------------------------===//
// Register classes
//===----------------------------------------------------------------------===//

// 后端框架好像需要至少一个寄存器类，所以定义一个意思意思
def RC : RegisterClass<
  "Gyh",      // 命名空间
  [i32],      // 数据类型
  32,         // 寄存器位宽
  (add R0)    // 这是一个 dag 类型的参数，要写一个集合表达式
              //（add R0) 就表示包含 R0 一个元素的集合
>;
